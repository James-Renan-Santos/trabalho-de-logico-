programa {
  funcao inicio() {
// Declaração de Variáveis.
  real graus, fahrenheit

// Entrada de Dados.
  escreva (" Informe a temperatura em °C: ")
  leia (graus)

// Processamento.
  fahrenheit= (9 * graus + 160) /5

// Saída de Dados.
  escreva ( graus, "°C" , "\n em Fahrenheit, é a: " , fahrenheit , "°F.")
  }
}
