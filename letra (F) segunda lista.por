programa {
  funcao inicio() {
// Delcaração de Variáveis.
  real raio , altura , volume
// Entrada de Dados.
  escreva (" Informe a altura: ")
  leia (altura)
  escreva (" informe quanto vale o raio: ")
  leia (raio)
// Processamento.
  volume = 3.14 * raio * altura
// Saída de Dados.
  escreva ("o Volume é de\n:" , volume )

  }
}
