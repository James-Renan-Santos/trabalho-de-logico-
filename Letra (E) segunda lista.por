programa {
  funcao inicio() {
// Declaração de Variáveis.
  real graus, fahrenheit

// Entrada de Dados.
  escreva (" Informe a temperatura em °fahrenheit: ")
  leia (fahrenheit)

// Processamento.
  graus = ((fahrenheit-32)*5)/9

// Saída de Dados.
  escreva ( fahrenheit, "°F" , "\n em Ceus e a: " , graus , "°C.")
  }
}
